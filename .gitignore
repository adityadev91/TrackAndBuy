# See https://help.github.com/ignore-files/ for more about ignoring files.
# Dependencies
graphql_server/node_modules/

# System Files
.DS_Store

# Client specific file to be generated by script
graphql_server/vars.env

# Temporary files that get generated on execution of setup_and_rn.sh
master
graphql_server/etc/
client/extensions/
client/cache/
client/specifications/
client/doc/
client/gems/
client/build_info/
client/bin/thor
client/.bundle/

# Ignore the default SQLite database.
client/db/*.sqlite3
client/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
client/log/*
client/tmp/*
!client/log/.keep
!client/tmp/.keep

# Ignore uploaded files in development
client/storage/*
!client/storage/.keep

client/node_modules
client/yarn-error.log

client/public/assets
client.byebug_history

# Ignore master key for decrypting credentials and more.
client/config/master.key
