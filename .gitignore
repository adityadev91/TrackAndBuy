# See https://help.github.com/ignore-files/ for more about ignoring files.
# Dependencies
graphql_server/node_modules/

# System Files
.DS_Store

# Environment variable values to be generated by script
graphql_server/vars.env

# Temporary files that get generated on execution of setup_and_rn.sh
master
graphql_server/etc/
rails_app/extensions/
rails_app/cache/
rails_app/specifications/
rails_app/doc/
rails_app/gems/
rails_app/build_info/
rails_app/bin/thor
rails_app/.bundle/

# Ignore the default SQLite database.
rails_app/db/*.sqlite3
rails_app/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
rails_app/log/*
rails_app/tmp/*
!rails_app/log/.keep
!rails_app/tmp/.keep

# Ignore uploaded files in development
rails_app/storage/*
!rails_app/storage/.keep

rails_app/node_modules
rails_app/yarn-error.log

rails_app/public/assets
rails_app.byebug_history

# Ignore master key for decrypting credentials and more.
rails_app/config/master.key
